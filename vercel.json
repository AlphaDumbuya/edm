{
  "version": 2,
  "buildCommand": "npm run vercel-build",
  "framework": "nextjs",
  "crons": [
    {
      "path": "/api/cron/event-reminders",
      "schedule": "0 6 * * *"
    }
  ],
  "env": {
    "NEXT_PUBLIC_VERCEL_ENV": "production"
  },
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  }
}
