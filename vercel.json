{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "npm run vercel-build",
  "framework": "nextjs",
  "env": {
    "NEXT_PUBLIC_VERCEL_ENV": "production"
  },
  "crons": [
    {
      "path": "/api/process-reminders",
      "schedule": "* * * * *"
    },
    {
      "path": "/api/reminders/process",
      "schedule": "* * * * *"
    }
  ]
}
