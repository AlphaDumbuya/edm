{
  "version": 2,
  "buildCommand": "npm run vercel-build",
  "framework": "nextjs",
  "env": {
    "NEXT_PUBLIC_VERCEL_ENV": "production"
  },
  "functions": [
    {
      "path": "/api/process-reminders",
      "schedule": "* * * * *"
    },
    {
      "path": "/api/reminders/process",
      "schedule": "* * * * *"
    }
  ]
}
